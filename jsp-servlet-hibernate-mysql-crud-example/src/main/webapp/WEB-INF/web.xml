<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
 
<web-app>
    <display-name>Archetype Created Web Application</display-name>
       <servlet>
        <servlet-name>jsp-servlet-hibernate-mysql-crud-example</servlet-name>
         <servlet-class>jsp-servlet-hibernate-mysql-crud-example</servlet-class>
        <init-param>
            <param-name>jsp-servlet-hibernate-mysql-crud-example</param-name>
            <param-value>net.javaguides.usermanagement.resource</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
       </servlet>
 	<servlet-mapping>
        <servlet-name>jsp-servlet-hibernate-mysql-crud-example</servlet-name>
        <url-pattern>/rest/*</url-pattern>
    </servlet-mapping>
    
</web-app>
